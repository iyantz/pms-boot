<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yantz.boot.project.mapper.PmsRequirementMapper">

    <!-- 获取需求信息分页列表 -->
    <select id="getPmsRequirementPage" resultType="com.yantz.boot.project.model.vo.PmsRequirementVO">
        SELECT
                project_id,
                requirement_name,
                proposer,
                propose_time,
                expect_finish_time,
                status,
                description,
                create_time,
                update_time
        FROM
            pms_requirement
        <where>
        </where>
    </select>

</mapper>
