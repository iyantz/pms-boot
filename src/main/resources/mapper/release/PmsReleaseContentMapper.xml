<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yantz.boot.release.mapper.PmsReleaseContentMapper">

    <!-- 获取发布内容分页列表 -->
    <select id="getPmsReleaseContentPage" resultType="com.yantz.boot.release.model.vo.PmsReleaseContentVO">
        SELECT
                content_id,
                release_id,
                content,
                file_path,
                create_by,
                create_time,
                update_by,
                update_time,
                is_deleted
        FROM
            pms_release_content
        <where>
        </where>
    </select>

</mapper>
